#ifndef _OPENH323_BUILDOPTS_H
#define _OPENH323_BUILDOPTS_H

// PTLIB VERSION

#define  PT_MAJOR_VERSION   2
#define  PT_MINOR_VERSION   14
#define  PT_BUILD_NUMBER    4

#if PT_MAJOR_VERSION == 2 && PT_MINOR_VERSION < 10
   #define PTLIB_VER ( PT_MAJOR_VERSION*100 + PT_MINOR_VERSION*10 + PT_BUILD_NUMBER )
#else
   #if (PTLIB_BUILD < 10)  // We currently only handle Build numbers less then 10 - SH
      #define PTLIB_VER ( PT_MAJOR_VERSION*1000 + PT_MINOR_VERSION*10 + PT_BUILD_NUMBER )
   #else
      #define PTLIB_VER ( PT_MAJOR_VERSION*1000 + PT_MINOR_VERSION*10 + 9 )
   #endif 
#endif

